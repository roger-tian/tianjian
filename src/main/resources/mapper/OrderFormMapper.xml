<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.leisu.tianjian.dao.OrderFormDao" >
    <resultMap id="BaseResultMap" type="com.leisu.tianjian.model.OrderFormModel" >
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="from" property="from" jdbcType="VARCHAR" />
        <result column="to" property="to" jdbcType="VARCHAR" />
        <result column="weight" property="weight" jdbcType="VARCHAR" />
        <result column="volume" property="volume" jdbcType="VARCHAR" />
        <result column="flag" property="flag" jdbcType="VARCHAR" />
        <result column="amount" property="amount" jdbcType="VARCHAR" />
    </resultMap>

    <sql id="Base_Column_List" >
        id, from, to, weight, volume, flag, amount
    </sql>

    <select id="insert" resultType="String" parameterType="com.leisu.tianjian.model.OrderFormModel">
        insert into order_form(from, to, weight, volume, flag, amount)
                values(#{from}, #{to}, #{weight}, #{volume}, #{flag}, #{amount})
    </select>
</mapper>